{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cmake",
			"label": "CMake: build",
			"command": "build",
			"targets": [
				"ALL_BUILD"
			],
			"group": "build",
			"problemMatcher": [],
			"detail": "CMake template build task"
		},
		{
			"type": "cppbuild",
			"label": "C/C++: cl.exe build active file",
			"command": "cl.exe",
			"args": [
					"/Zi",
					"/EHsc",
					"/O2",
					"/nologo",
					"/Fo${fileDirname}\\target\\obj\\",
					"/Fe${fileDirname}\\target\\${fileBasenameNoExtension}.exe",
					"${file}",
					"/I",
					"external\\sdl3\\include",
					"/link",
					"/LIBPATH:external\\sdl3\\build\\RelWithDebInfo",
					"SDL3.lib",
			],
			"options": {
					"cwd": "${fileDirname}"
			},
			"problemMatcher": [
					"$msCompile"
			],
			"group": {
					"kind": "build",
					"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	]
}